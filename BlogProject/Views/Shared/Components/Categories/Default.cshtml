@model List<Category>


<div class="list-group">
    <a href="/blogs" class="list-group-item list-group-item-action">Tum Kategoriler</a>
    @foreach (var category in Model)
    {
        <a asp-controller="Blog" asp-action="List" asp-route-category="@category.Url"
       class="list-group-item list-group-item-action @(ViewBag.SelectedCategory == category.Url ? "active" : "") ">@category.CategoryName</a>
    }
</div>
